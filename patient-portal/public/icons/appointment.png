<svg width="192" height="192" viewBox="0 0 192 192" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="appointmentGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00ff00"/>
      <stop offset="100%" style="stop-color:#00ffff"/>
    </linearGradient>
    <filter id="appointmentGlow">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>

  <!-- Calendar background -->
  <rect x="25" y="40" width="142" height="112" rx="15" ry="15" fill="url(#appointmentGradient)" opacity="0.2"/>
  <rect x="25" y="25" width="142" height="15" rx="15" ry="15" fill="url(#appointmentGradient)" filter="url(#appointmentGlow)"/>

  <!-- Calendar lines -->
  <g stroke="url(#appointmentGradient)" stroke-width="2" opacity="0.8">
    <line x1="45" y1="60" x2="157" y2="60"/>
    <line x1="45" y1="80" x2="157" y2="80"/>
    <line x1="45" y1="100" x2="157" y2="100"/>
    <line x1="45" y1="120" x2="157" y2="120"/>
    <line x1="75" y1="40" x2="75" y2="132"/>
    <line x1="105" y1="40" x2="105" y2="132"/>
    <line x1="135" y1="40" x2="135" y2="132"/>
  </g>

  <!-- Clock -->
  <circle cx="96" cy="90" r="20" fill="none" stroke="url(#appointmentGradient)" stroke-width="3" filter="url(#appointmentGlow)"/>
  <line x1="96" y1="90" x2="96" y2="78" stroke="url(#appointmentGradient)" stroke-width="2" stroke-linecap="round"/>
  <line x1="96" y1="90" x2="105" y2="95" stroke="url(#appointmentGradient)" stroke-width="2" stroke-linecap="round"/>
</svg>