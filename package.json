{
  "name": "dentiagest",
  "version": "1.0.0",
  "description": "DentiaGest es un sistema completo de gestión para clínicas dentales, desarrollado con tecnologías modernas para ofrecer una experiencia profesional y eficiente.",
  "type": "module",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:selene": "cd selene && npm run build",
    "build:dashboard": "cd dashboard-new && npm run build",
    "sync:backend": "node sync-backend-to-dashboard.cjs",
    "build:all": "npm run build:selene && npm run sync:backend",
    "resurrection-protocol": "node resurrection-protocol.js",
    "validate": "node validate-anti-simulation.cjs",
    "validate:watch": "node .hooks/validation-guardian.cjs",
    "validate:fix": "node scripts/fix-math-random.cjs",
    "prebuild": "npm run validate",
    "lint": "npx eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "npx eslint src --ext .ts,.tsx,.js,.jsx",
    "chaos:test": "node -e \"import('./selene/src/consciousness/engines/DreamForgeEngine.ts').then(m => m.DreamForgeEngine.runChaosValidation()).catch(console.error)\"",
    "chaos:test:memory": "node -e \"import('./selene/src/consciousness/engines/DreamForgeEngine.ts').then(m => m.DreamForgeEngine.runMemoryStressTest()).catch(console.error)\"",
    "chaos:test:timeout": "node -e \"import('./selene/src/consciousness/engines/DreamForgeEngine.ts').then(m => m.DreamForgeEngine.runTimeoutChaosTest()).catch(console.error)\"",
    "chaos:test:circuit-breaker": "node -e \"import('./selene/src/consciousness/engines/DreamForgeEngine.ts').then(m => m.DreamForgeEngine.runCircuitBreakerTest()).catch(console.error)\"",
    "chaos:test:pool-overload": "node -e \"import('./selene/src/consciousness/engines/DreamForgeEngine.ts').then(m => m.DreamForgeEngine.runPoolOverloadTest()).catch(console.error)\"",
    "chaos:test:recovery": "node -e \"import('./selene/src/consciousness/engines/DreamForgeEngine.ts').then(m => m.DreamForgeEngine.runRecoveryChaosTest()).catch(console.error)\"",
    "chaos:test:meta-consciousness": "node -e \"import('./selene/src/consciousness/engines/DreamForgeEngine.ts').then(m => m.DreamForgeEngine.runMetaConsciousnessStressTest()).catch(console.error)\"",
    "chaos:ci": "npm run chaos:test:memory && npm run chaos:test:timeout && npm run chaos:test:circuit-breaker && npm run chaos:test:pool-overload && npm run chaos:test:recovery && npm run chaos:test:meta-consciousness",
    "test:chaos": "npm run chaos:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinkyfloyder/Dentiagest.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pinkyfloyder/Dentiagest/issues"
  },
  "homepage": "https://github.com/pinkyfloyder/Dentiagest#readme",
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express4": "^1.1.2",
    "@graphql-tools/schema": "^10.0.25",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@types/bcrypt": "^6.0.0",
    "@types/chai": "^5.2.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/web3": "^1.0.20",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.18.1",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "bull": "^4.16.5",
